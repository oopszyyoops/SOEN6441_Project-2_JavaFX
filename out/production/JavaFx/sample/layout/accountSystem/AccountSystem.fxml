<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="342.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label layoutX="70.0" layoutY="39.0" text="Account System">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="70.0" layoutY="86.0" mnemonicParsing="false" onAction="#changePaymentPaid" text="1. Change payment to paid" />
      <Button layoutX="70.0" layoutY="125.0" mnemonicParsing="false" onAction="#changePaymentUnpaid" text="2. Change payment to unpaid" />
      <Button layoutX="70.0" layoutY="168.0" mnemonicParsing="false" onAction="#displayPaidList" text="3. Display paid list" />
      <Button layoutX="70.0" layoutY="215.0" mnemonicParsing="false" onAction="#displayUnpaidList" text="4. Display unpaid list" />
      <Button layoutX="247.0" layoutY="274.0" mnemonicParsing="false" onAction="#goBacktoMainMenu" text="Exit" />
   </children>
</AnchorPane>
